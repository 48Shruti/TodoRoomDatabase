<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.shruti.todoroomdatabase.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_subTaskFragment"
            app:destination="@id/subTaskFragment" />
    </fragment>
    <fragment
        android:id="@+id/subTaskFragment"
        android:name="com.shruti.todoroomdatabase.SubTaskFragment"
        android:label="fragment_sub_task"
        tools:layout="@layout/fragment_sub_task" />
</navigation>